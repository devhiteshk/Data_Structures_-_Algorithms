     "execution_count": 4,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# Brute force method is to run a loop till M/N and check each case\n",
    "\n",
    "def getNthRoot(N,M): # T.C. = O(N) & S.C O(1)\n",
    "    \n",
    "    Range = M//N + 1\n",
    "    \n",
    "    for i in range(Range):\n",
    "        if i**N == M:\n",
    "            return i\n",
    "        \n",
    "    return -1\n",
    "\n",
    "getNthRoot(2,16)\n",
    "        "
   ]
