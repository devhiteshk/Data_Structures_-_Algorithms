    "# Optimized method is to use Binary Search\n",
    "\n",
    "def getNthRootBS(N,M): # T.C. O(log(N)) & S.C O(1)\n",
    "    \n",
    "    if M == 1:\n",
    "        return 1\n",
    "    \n",
    "    R = M//N + 1\n",
    "    \n",
    "    low = 1\n",
    "    high = R\n",
    "    \n",
    "    while low <= high:\n",
    "        \n",
    "        mid = (low + high)//2\n",
    "        \n",
    "        if mid**N == M:\n",
    "            return mid\n",
    "        \n",
    "        elif mid**N > M:\n",
    "            high = mid - 1\n",
    "            \n",
    "        else:\n",
    "            low = mid + 1\n",
    "            \n",
    "    return -1\n",
    "        \n",
    "\n",
    "getNthRootBS(2,81)"
   ]
